<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<environment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" id="c4af0570-f881-4c9a-966d-6559bb30fe2a" name="Default Environment" description="Environment that contains the default inspectIT monitoring settings and all default profiles." created-on="2019-03-14T09:45:33.499Z" updated-on="2019-03-14T10:04:08.197Z" revision="3" schemaVersion="9" xsi:noNamespaceSchemaLocation="../schema/ciSchema.xsd">
    <end-user-monitoring-config eum-enabled="true" eum-script-base-url="/scripts/" active-modules="mablr12" relevancyThreshold="0" listenerInstrumentationAllowed="true" agentMinificationEnabled="true"/>
    <platform-sensor-configs>
        <class-loading-sensor-config active="true"/>
        <compilation-sensor-config active="true"/>
        <cpu-sensor-config active="true"/>
        <memory-sensor-config active="true"/>
        <runtime-sensor-config active="true"/>
        <system-sensor-config active="true"/>
        <thread-sensor-config active="true"/>
    </platform-sensor-configs>
    <method-sensor-configs>
        <connection-sensor-config/>
        <http-sensor-config sessionCapture="false" attributesCapture="false" parametersCapture="false" string-length="500"/>
        <invocation-sequence-sensor-config string-length="100"/>
        <prepared-statement-parameter-sensor-config/>
        <prepared-statement-sensor-config string-length="1000"/>
        <statement-sensor-config string-length="1000"/>
        <timer-sensor-config string-length="100"/>
        <log4j-logging-sensor-config minLevel="WARN"/>
        <remote-apache-httpclientV40-client-sensor-config/>
        <remote-jetty-httpclientV61-client-sensor-config/>
        <remote-urlconnection-client-sensor-config/>
        <remote-spring-resttemplate-client-sensor-config/>
        <remote-http-server-sensor-config/>
        <remote-jms-client-sensor-config/>
        <remote-jms-listener-server-sensor-config/>
        <remote-manual-server-sensor-config/>
        <executor-client-sensor-config/>
        <remote-async-apache-httpclient-sensor-config/>
        <apache-client-exchange-handler-sensor/>
    </method-sensor-configs>
    <exception-sensor-config enhanced="false" string-length="500"/>
    <jmx-loading-sensor-config active="true" forceMBeanServerCreation="false"/>
    <profile-ids>
        <profile-id>remote-http</profile-id>
        <profile-id>exclude-classes</profile-id>
        <profile-id>remote-jms</profile-id>
        <profile-id>executor-correlation</profile-id>
        <profile-id>http</profile-id>
        <profile-id>sql</profile-id>
    </profile-ids>
    <classLoadingDelegation>true</classLoadingDelegation>
    <retransformation-strategy>DISABLE_ON_IBM_JVM</retransformation-strategy>
    <disruptor-strategy-config buffer-size="1024"/>
</environment>
